<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.germanclone">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Phases"
            android:parentActivityName=".MainActivity"
            android:label="@string/phrases"></activity>
        <activity android:name=".Colors"
            android:label="@string/color" android:parentActivityName=".MainActivity" ></activity>
        <activity android:name=".FamilyMembers"
            android:label="@string/family" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".Numbers" android:parentActivityName=".MainActivity"
            android:label="@string/Number"></activity>
    </application>

</manifest>